# copy&rename to .env.local

# Do not leave empty values, e.g. `NEXT_OUTPUT=`, as it is interpreted as an empty string and
# default values won't be used. If you want to fallback to default values, remove or comment the line completely.

# -------------------------- Next.js settings ---------------------------------

# See next.config.js's "output" option. Possible values:
# - "standalone" - docker container
# - "export" - static export
# - undefined - default, `next start`
NEXT_OUTPUT=standalone

# Revalidate interval (false | 0 | number). Default: false - infinity (Nextjs 14)
# https://nextjs.org/docs/14/app/api-reference/file-conventions/route-segment-config#revalidate
# NEXT_PUBLIC_REVALIDATE=

# Sets `cache` type to either in memory or the file system. Filesystem cache is used by default.
# https://webpack.js.org/configuration/cache/?utm_source=chatgpt.com#cachetype
# WEBPACK_CACHE_TYPE=

# -------------------------- NextAuth settings ---------------------------------

# Base next-auth URL - canonical URL of site
# https://next-auth.js.org/configuration/options#nextauth_url
NEXTAUTH_URL=http://localhost:3000

# Encrypt JWT
# https://next-auth.js.org/configuration/options#nextauth_secret
NEXTAUTH_SECRET=password_secret

# -------------------------- BE settings ---------------------------------

# URL of Strapi
STRAPI_URL=http://127.0.0.1:1337

# Read Only API KEY generated within Strapi, providing access to find and findOne endpoints
STRAPI_REST_READONLY_API_KEY=

# API KEY with custom permissions, providing different access to Strapi endpoints
STRAPI_REST_CUSTOM_API_KEY=

# -------------------------- Sentry settings ---------------------------------

# Sentry DSN
# NEXT_PUBLIC_SENTRY_DSN=

# Sentry auth token
# SENTRY_AUTH_TOKEN=

# Sentry organization
# SENTRY_ORG=

# Sentry project
# SENTRY_PROJECT=

SENTRY_SUPPRESS_GLOBAL_ERROR_HANDLER_FILE_WARNING=1

# -------------------------- ReCAPTCHA v3 ---------------------------------

# NEXT_PUBLIC_RECAPTCHA_SITE_KEY=

# RECAPTCHA_SECRET_KEY=

# -------------------------- App settings ---------------------------------

# canonical URL of site - base URL of the frontend application
# (it is used to navigate, generate sitemap URLs, canonical SEO tags etc.)
APP_PUBLIC_URL=http://localhost:3000

# Components and functions that are not *marked as used* will log a warning to the console during development by default.
# To change this behavior, set this env var to `true`.
# NEXT_PUBLIC_PREVENT_UNUSED_FUNCTIONS_ERROR_LOGS=true

# Strapi Preview Shared Secret
STRAPI_PREVIEW_SECRET=your-secret-key

# -------------------------- Stripe settings ---------------------------------

# Stripe API Keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Products (for reference)
STRIPE_PRODUCT_DAY_PASS=prod_...
STRIPE_PRODUCT_SOLO=prod_...
STRIPE_PRODUCT_STUDIO=prod_...
STRIPE_PRODUCT_AGENCY=prod_...
STRIPE_PRODUCT_LIFETIME=prod_...
STRIPE_PRODUCT_FAST_TURNAROUND=prod_...

# Stripe Prices - Day Pass
NEXT_PUBLIC_STRIPE_PRICE_DAY_PASS=price_...

# Stripe Prices - Solo Plan
NEXT_PUBLIC_STRIPE_PRICE_SOLO_MONTHLY=price_...
NEXT_PUBLIC_STRIPE_PRICE_SOLO_ANNUAL=price_...

# Stripe Prices - Studio Plan
NEXT_PUBLIC_STRIPE_PRICE_STUDIO_MONTHLY=price_...
NEXT_PUBLIC_STRIPE_PRICE_STUDIO_ANNUAL=price_...

# Stripe Prices - Agency Plan
NEXT_PUBLIC_STRIPE_PRICE_AGENCY_MONTHLY=price_...
NEXT_PUBLIC_STRIPE_PRICE_AGENCY_ANNUAL=price_...

# Stripe Prices - Lifetime
NEXT_PUBLIC_STRIPE_PRICE_LIFETIME_CORE=price_...
NEXT_PUBLIC_STRIPE_PRICE_LIFETIME_PLUS=price_...

# Stripe Prices - Add-ons
NEXT_PUBLIC_STRIPE_PRICE_FAST_TURNAROUND=price_...

# Stripe Configuration
STRIPE_SUCCESS_URL=${APP_PUBLIC_URL}/account/thanks
STRIPE_CANCEL_URL=${APP_PUBLIC_URL}/pricing
STRIPE_PORTAL_RETURN_URL=${APP_PUBLIC_URL}/account/billing
STRIPE_TAX_ENABLED=false
STRIPE_CUSTOMER_PORTAL_ENABLED=true

# Payment Grace Period
PAYMENT_GRACE_PERIOD_DAYS=3

# -------------------------- Cloudflare settings ---------------------------------

# Cloudflare Account
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=your-api-token

# R2 Storage
R2_ACCESS_KEY_ID=your-r2-access-key
R2_SECRET_ACCESS_KEY=your-r2-secret-key
R2_BUCKET_NAME_DEV=framer-templates-dev
R2_BUCKET_NAME_STAGING=framer-templates-staging
R2_BUCKET_NAME_PROD=framer-templates-prod
R2_REGION=auto
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com

# R2 Signed URLs
R2_SIGNED_URL_EXPIRY_SECONDS=900
R2_MAX_RETRY_ATTEMPTS=3

# Worker
WORKER_URL=https://api.yourdomain.com
WORKER_ENV=development

# Pages
PAGES_PROJECT_NAME=framer-templates-ui

# -------------------------- Email Service ---------------------------------

# Provider (sendgrid | postmark | ses | smtp)
EMAIL_PROVIDER=sendgrid

# SendGrid
SENDGRID_API_KEY=SG...
SENDGRID_FROM_EMAIL=noreply@yourdomain.com
SENDGRID_FROM_NAME="Framer Templates"

# Email Templates
SENDGRID_TEMPLATE_WELCOME=d-...
SENDGRID_TEMPLATE_VERIFY=d-...
SENDGRID_TEMPLATE_RESET=d-...
SENDGRID_TEMPLATE_PAYMENT_FAILED=d-...
SENDGRID_TEMPLATE_PAYMENT_SUCCESS=d-...
SENDGRID_TEMPLATE_DOWNLOAD_RECEIPT=d-...
SENDGRID_TEMPLATE_REQUEST_RECEIVED=d-...
SENDGRID_TEMPLATE_REQUEST_UPDATE=d-...

# -------------------------- Feature Flags ---------------------------------

NEXT_PUBLIC_FEATURE_DOWNLOADS=true
NEXT_PUBLIC_FEATURE_TEMPLATE_REQUESTS=true
NEXT_PUBLIC_FEATURE_FAVORITES=true
NEXT_PUBLIC_FEATURE_BULK_DOWNLOAD=false
