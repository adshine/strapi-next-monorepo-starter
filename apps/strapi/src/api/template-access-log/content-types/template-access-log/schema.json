{
  "kind": "collectionType",
  "collectionName": "template_access_logs",
  "info": {
    "singularName": "template-access-log",
    "pluralName": "template-access-logs",
    "displayName": "TemplateAccessLog",
    "description": "Track all template remix attempts and completions"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project"
    },
    "accessId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "success", "failed", "expired"],
      "default": "pending",
      "required": true
    },
    "initiatedAt": {
      "type": "datetime",
      "required": true
    },
    "completedAt": {
      "type": "datetime"
    },
    "expiresAt": {
      "type": "datetime",
      "required": true
    },
    "remixUrl": {
      "type": "text"
    },
    "remixUrlHash": {
      "type": "string"
    },
    "issuedAt": {
      "type": "datetime"
    },
    "ipAddress": {
      "type": "string"
    },
    "userAgent": {
      "type": "text"
    },
    "sourceIp": {
      "type": "string"
    },
    "attemptNumber": {
      "type": "integer",
      "default": 1,
      "min": 1,
      "max": 3
    },
    "retryOf": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::template-access-log.template-access-log"
    },
    "quotaCharged": {
      "type": "boolean",
      "default": false
    },
    "errorReason": {
      "type": "text"
    },
    "errorCode": {
      "type": "string"
    },
    "supportTicketId": {
      "type": "string"
    },
    "accessDuration": {
      "type": "integer"
    },
    "templateSize": {
      "type": "biginteger"
    },
    "metadata": {
      "type": "json"
    }
  }
}